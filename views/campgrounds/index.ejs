<% layout('layouts/boilerplate') %>
<h1>All Campgorunds</h1>
    <header>
        <a href="/campgrounds/new">Add Campground</a>
    </header>
    <ul>
        <% for(let campground of campgrounds){ %>
            <li>
                <a href="/campgrounds/<%= campground._id %> ">
                    <%= campground.title %>
                </a>
            </li>
            <% } %>
    </ul>